// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Inventory {
  serial_number String  @unique
  brand_name    String
  volume        String
  retail_price  Float
  on_sale       Boolean
  sale_price    Float?
}

model Logos {
  logoId   Int    @id @default(autoincrement())
  imageUrl String
}
